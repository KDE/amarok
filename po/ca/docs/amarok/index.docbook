<sect1 id="database"
><title
>Base de dades</title
> 
<para>
<screenshot>
<screeninfo
></screeninfo>
  <mediaobject>
    <imageobject>
      <imagedata fileref="amarok_2-8_configurationdialogdatabase.png" format="PNG"/>
